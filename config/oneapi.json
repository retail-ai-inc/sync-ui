    "/api/sync/{id}/metrics": {
      "get": {
        "tags": ["sync"],
        "description": "获取同步任务的指标数据",
        "operationId": "getSyncMetrics",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "同步任务的ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "range",
            "in": "query",
            "description": "时间范围（例如：1h, 3h, 6h, 12h, 1d, 2d, 7d, custom）",
            "schema": {
              "type": "string",
              "enum": ["1h", "3h", "6h", "12h", "1d", "2d", "7d", "custom"]
            }
          },
          {
            "name": "startTime",
            "in": "query",
            "description": "自定义时间范围的开始时间（ISO 8601格式）",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "endTime",
            "in": "query",
            "description": "自定义时间范围的结束时间（ISO 8601格式）",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "成功获取指标数据",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SyncMetricsResponse"
                }
              }
            }
          },
          "401": {
            "description": "错误响应",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
